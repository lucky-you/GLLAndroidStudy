<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MarqueeView">
        <attr name="standDuration" format="integer|reference" />
        <attr name="turningDuration" format="integer|reference" />
        <attr name="showItemCount" format="integer|reference" />
        <attr name="scrollItemCount" format="integer|reference" />
    </declare-styleable>


    <!--签到的View-->
    <declare-styleable name="SignInStepView">
        <!--圆的颜色-->
        <attr name="svCircleColor" format="color" />
        <!--字体的颜色-->
        <attr name="svTextColor" format="color" />
        <!--字体被选中时的颜色-->
        <attr name="svSelectedColor" format="color" />
        <!--圆角-->
        <attr name="svFillRadius" format="dimension" />
        <!--间距的宽度-->
        <attr name="svStrokeWidth" format="dimension" />
        <!--线宽-->
        <attr name="svLineWidth" format="dimension" />
        <!--字体大小-->
        <attr name="svTextSize" format="dimension" />
        <!--间距-->
        <attr name="svDrawablePadding" format="dimension" />

    </declare-styleable>


    <declare-styleable name="CustomTitleView">
        <attr name="cusTextViewText" format="string" />
        <attr name="cusTextViewColor" format="color" />
        <attr name="cusTextViewSize" format="dimension" />

    </declare-styleable>


    <declare-styleable name="MomentPicView">
        <attr name="mpvHorizontalSpace" format="dimension" />
        <attr name="mpvVerticalSpace" format="dimension" />
        <attr name="mpvRadius" format="dimension" />
        <attr name="mpvRatio" format="float" />
        <attr name="mpvColumns" format="integer" />
        <attr name="mpvPrevNum" format="integer" />
        <attr name="mpvTest" format="boolean" />
        <attr name="mpvShowRule" format="enum">
            <enum name="RULE_123" value="1" />
            <enum name="RULE_13" value="2" />
            <enum name="RULE_3" value="3" />
            <enum name="RULE_1" value="4" />
        </attr>
        <attr name="mpvAddIcon" format="reference" />
        <attr name="mpvAddIconInStart" format="boolean" />
        <attr name="mpvDelIcon" format="reference" />
        <attr name="mpvMaxImg" format="integer" />
    </declare-styleable>
</resources>